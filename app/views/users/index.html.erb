<%= render 'books/list', user: @user, book: @book %>
<h2>Users</h2>
<table>
	<tr>
		<th>image</th>
		<th>name</th>
	</tr>
	<% @users.each do |user| %>
	<tr>
		<td><%= attachment_image_tag user,
		:profile_image, :fill, 35, 35,
		format: 'jpeg', fallback: "no_image.jpg" %>
		<td><%= user.name %></td>
		<td><%= link_to "Show", user_path(current_user.id) %></td>
	</tr>

	<% end %>
</table>
